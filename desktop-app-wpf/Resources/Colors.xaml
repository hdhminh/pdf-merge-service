<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ColorBackground">#F3F5F7</Color>
    <Color x:Key="ColorPrimaryText">#0F2C3F</Color>
    <Color x:Key="ColorSubtleText">#4F6677</Color>
    <Color x:Key="ColorBorder">#D6DDE3</Color>

    <SolidColorBrush x:Key="BrushBackground" Color="{StaticResource ColorBackground}" />
    <SolidColorBrush x:Key="BrushPrimaryText" Color="{StaticResource ColorPrimaryText}" />
    <SolidColorBrush x:Key="BrushSubtleText" Color="{StaticResource ColorSubtleText}" />
    <SolidColorBrush x:Key="BrushBorder" Color="{StaticResource ColorBorder}" />
</ResourceDictionary>
